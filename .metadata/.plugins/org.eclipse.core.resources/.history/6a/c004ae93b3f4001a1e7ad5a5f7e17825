public class AccountTest3 {

	public static void main(String[] args) {
		//create objects
		java.util.Scanner input = new java.util.Scanner(System.in);
		
	   //typeAndNameOfClass  objectName = newToCreatObject defaultConstructor 
		Account3 myAccount3 = new Account3();//constructor
		System.out.println("YOUR CONSTRUCTOR NAME IS: " + myAccount3.getName());
		
		System.out.println("your constructor age by default is: " + myAccount3.getAge());
		
		System.out.println("Enter your name: ");
		String name = input.next();
		myAccount3.setName(name);
		
		System.out.printf("Welcome %s you changed your constructor name!  %n", myAccount3.getName());
		
		
	//	myAccount3.setName("asma");
		
